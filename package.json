{
  "name": "json-schema-to-dts",
  "version": "0.1.0",
  "description": "Convert JSON Schema to TypeScript types with JSDoc annotations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build && pnpm test"
  },
  "keywords": [
    "json-schema",
    "typescript",
    "types",
    "jsdoc"
  ],
  "engines": {
    "node": ">=14.14.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "author": "",
  "license": "MIT",
  "volta": {
    "node": "20.20.0",
    "pnpm": "10.28.0"
  },
  "devDependencies": {
    "@types/node": "20",
    "@vitest/coverage-v8": "^4.0.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "glob": "^13.0.0"
  }
}
